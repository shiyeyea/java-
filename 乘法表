package com.shang.basis.struct;

public class forstudy {
    public static void main(String[] args) {
        for (int i = 1; i <= 9; i++) {
            for (int i1 = 1; i1 <=i; i1++) {
                int sum=i*i1;
                System.out.print( i+"*"+i1+"="+sum+"\t");//\t 换行
            }
            System.out.println();//换行
        }
    }
}
